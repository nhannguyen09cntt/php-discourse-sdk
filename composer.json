{
  "name": "nhannguyen09cntt/php-discourse-sdk",
  "type": "library",
  "description": "Discourse SDK for PHP",
  "keywords": ["discourse","php","4rum.vn"],
  "homepage": "https://github.com/nhannguyen09cntt/php-discourse-sdk",
  "license": "MIT",
  "authors": [
      {
          "name": "Nhan Nguyen",
          "email": "nguyennhan09cntt@gmail.com",
          "homepage": "http://nhannvt.com",
          "role": "Developer"
      }
  ],
  "require": {
    "php": "^7.0"
  },
  "require-dev": {
      "phpunit/phpunit": "^8.0",
      "mockery/mockery": "^1.0",
      "guzzlehttp/guzzle": "^7.0"
  },
  "autoload": {
      "psr-4": {
          "Discourse\\": "src/Discourse/"
      },
      "files": ["src/Discourse/polyfills.php"]
  },
  "autoload-dev": {
      "psr-4": {
          "Discourse\\Tests\\": "tests/"
      }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}